<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

</div>
<!DOCTYPE html>
<html>
<head>
  <title><%= yield :title %> | Meu Site</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application", defer: true %>
</head>
<body>
  <%= "shared/navbar" %>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
